                            <v1
                                jcr:primaryType="nt:unstructured"
                                text="Tab 1"
                                value="tab1"/>
                            <v2
                                jcr:primaryType="nt:unstructured"
                                text="Tab 2"
                                value="tab2"/>
                            <v3
                                jcr:primaryType="nt:unstructured"
                                text="Tab 3"
