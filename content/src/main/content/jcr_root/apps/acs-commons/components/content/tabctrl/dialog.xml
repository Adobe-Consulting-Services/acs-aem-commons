                        fieldLabel="Selection"
                        name="./selection"
                        type="select"
                        xtype="selection">
                        <listeners
                            jcr:primaryType="nt:unstructured"
                            loadcontent="function(field,rec,path){Ejst.x2.showTab(field);}"
