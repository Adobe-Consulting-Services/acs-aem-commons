                                        text="Default Style"
                                        value=""/>
                                    <v2
                                        jcr:primaryType="nt:unstructured"
                                        text="Overlay Style"
                                        value="overlay"/>
                                </options>
                            </classselection>
                        </items>
                    </bannermode>
                </items>
            </selection>
