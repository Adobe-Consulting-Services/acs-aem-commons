<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Create PWA Configuration"
            sling:resourceType="cq/gui/components/siteadmin/admin/createpagewizard/page">
        <head jcr:primaryType="nt:unstructured">
            <viewport
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/admin/page/viewport"/>
            <favicon
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/page/favicon"/>
            <clientlibs
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/includeclientlibs"
                    categories="[coralui3,granite.ui.coral.foundation]"/>
        </head>
        <body
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/page/body">
            <items jcr:primaryType="nt:unstructured">
                <form
                        granite:class="cq-confadmin-create-acs-commons-pwa-item"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form"
                        action="/apps/acs-commons/cloudservices/pwa/content/configurations/createcloudconfigwizard/jcr:content"
                        foundationForm="{Boolean}true"
                        maximized="{Boolean}true"
                        method="post"
                        novalidate="{Boolean}true"
                        style="vertical">
                    <successresponse
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/responses/redirect"
                            href="/apps/acs-commons/cloudservices/pwa/content/configurations.html${granite:encodeURIPath(requestPathInfo.suffix)}"/>
                    <items jcr:primaryType="nt:unstructured">
                        <charset
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="_charset_"
                                value="utf-8"/>
                        <parentPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="parentPath"
                                value="${requestPathInfo.suffix}/settings/cloudconfigs"/>
                        <name
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="pageName"
                                value="pwa"/>
                        <page-description
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./jcr:description"
                                value="ACS Commons PWA cloud service configuration"/>
                        <tag-header
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentHeader"
                                value="acs-commons/cloudservices/pwa/components/scripttag/header"/>
                        <tag-header-rank
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentHeaderRank"
                                value="10"/>
                        <tag-header-rank-hint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentHeaderRank@TypeHint"
                                value="Long"/>
                        <tag-footer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentFooter"
                                value="acs-commons/cloudservices/pwa/components/scripttag/footer"/>
                        <tag-footer-rank
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentFooterRank"
                                value="10"/>
                        <tag-footer-rank-hint
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./tagComponentFooterRank@TypeHint"
                                value="Long"/>
                        <wizard
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Create PWA Configuration"
                                sling:resourceType="granite/ui/components/coral/foundation/wizard"
                                cancelHref="${empty header.Referer ? granite:concat(&quot;/apps/acs-commons/cloudservices/pwa/content/configurations.html&quot;, granite:encodeURIPath(requestPathInfo.suffix)) : header.Referer}">
                            <items jcr:primaryType="nt:unstructured">
                                <service-worker-wizard-container
                                        granite:class="general"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Service Worker"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        maximized="{Boolean}true">
                                    <items jcr:primaryType="nt:unstructured">
                                        <fixedcolumns
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                maximized="{Boolean}true">
                                            <items jcr:primaryType="nt:unstructured">
                                                <container
                                                        granite:class="properties"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <general
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <service-worker-js
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                        emptyText="Service Worker Client Library"
                                                                        fieldDescription="The AEM Client Library that loads the Service Worker JS."
                                                                        fieldLabel="Service Worker JavaScript"
                                                                        types="[service-worker]"
                                                                        name="./serviceWorkerJsClientLibraryCategory"
                                                                        required="{Boolean}true">
                                                                    <datasource
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="acs-commons/cloudservices/pwa/data-sources/client-library-categories"/>
                                                                </service-worker-js>
                                                                <scope
                                                                        granite:id="pwa-cloudconfiguration-version"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldLabel="Service Worker Scope"
                                                                        fieldDescription="A URL that defines a service worker\'s registration scope.relative to the base URL of the application. By default, the scope value for a service worker registration is set to the directory where the service worker script is located"
                                                                        name="./scope"
                                                                        value="/"
                                                                        required="{Boolean}true"/>
                                                                <version
                                                                        granite:id="pwa-cloudconfiguration-version"
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                        fieldLabel="Version Number"
                                                                        name="./version"
                                                                        min="1"
                                                                        value="1"
                                                                        defaultValue="1"
                                                                        required="{Boolean}true"/>

                                                                <fallbacks
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        fieldLabel="Fallback"
                                                                        fieldDescription="Click on Add Field to add more Fallback mappings. "
                                                                        composite="{Boolean}true"
                                                                        required="{Boolean}true">
                                                                    <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./fallback">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <pattern
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="Request Pattern"
                                                                                    name="./pattern"
                                                                                    required="{Boolean}true"/>
                                                                            <fallback-path
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="Fallback Path"
                                                                                    fieldDescription="The path to the fallback resource including the extension."
                                                                                    name="./path"
                                                                                    required="{Boolean}true"/>
                                                                        </items>
                                                                    </field>
                                                                </fallbacks>


                                                                <pre-caches
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        fieldLabel="Pre-cached URLs"
                                                                        fieldDescription="Click on Add Field to add more pre-cached resources. "
                                                                        composite="{Boolean}true"
                                                                        required="{Boolean}false">
                                                                    <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./pre-cache">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <url
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="URL to Pre-cache"
                                                                                    name="./url"
                                                                                    required="{Boolean}true"/>
                                                                        </items>
                                                                    </field>
                                                                </pre-caches>


                                                                <no-caches
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        fieldLabel="No Cache URL Patterns"
                                                                        fieldDescription="Click on Add Field to add more no-cache request patterns."
                                                                        composite="{Boolean}true"
                                                                        required="{Boolean}false">
                                                                    <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./no-cache">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <pattern
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                    fieldLabel="No-cache URL Patterns"
                                                                                    name="./url"
                                                                                    required="{Boolean}true"/>
                                                                        </items>
                                                                    </field>
                                                                </no-caches>


                                                                <resource-type
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                                        name="./sling:resourceType"
                                                                        value="acs-commons/cloudservices/pwa/components/conf/page"/>
                                                            </items>
                                                        </general>
                                                    </items>
                                                </container>
                                            </items>
                                        </fixedcolumns>
                                    </items>
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <next
                                                granite:class="foundation-wizard-control"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                                disabled="{Boolean}false"
                                                text="Next"
                                                variant="primary">
                                            <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                </service-worker-wizard-container>
                                <manifest-wizard-container
                                        granite:class="manifest"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="PWA Configuration"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        maximized="{Boolean}true">
                                    <items jcr:primaryType="nt:unstructured">
                                        <fixedcolumns
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                maximized="{Boolean}true">
                                            <items jcr:primaryType="nt:unstructured">
                                                <container
                                                        granite:class="properties"
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <name
                                                                granite:id="pwa-cloudconfiguration-name"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="The Application name to be displayed when the site launches from the splash screen"
                                                                fieldLabel="Application Name"
                                                                name="./jcr:title"
                                                                required="{Boolean}true"/>

                                                        <short-name
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldDescription="The Short name to be displayed when the site launches from the splash screen"
                                                                fieldLabel="Application Short Name"
                                                                name="./shortName"
                                                                required="{Boolean}true"/>

                                                        <pwa-js
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                emptyText="PWA Client Library"
                                                                fieldDescription="The AEM Client Library that loads the PWA JS."
                                                                fieldLabel="PWA JavaScript"
                                                                types="[pwa]"
                                                                name="./pwaJsClientLibraryCategory"
                                                                required="{Boolean}true">
                                                            <datasource
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="acs-commons/cloudservices/pwa/data-sources/client-library-categories"/>
                                                        </pwa-js>

                                                        <start-url
                                                                granite:id="pwa-cloudconfiguration-start-url"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                fieldDescription="Unless otherwise required. Keep '/'"
                                                                fieldLabel="Start URL"
                                                                name="./startPath"
                                                                required="{Boolean}true"
                                                                rootPath="/content"
                                                                value="/"/>

                                                        <theme-color
                                                                granite:id="pwa-cloudconfiguration-theme-color"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                                fieldLabel="Theme Color"
                                                                name="./themeColor"
                                                                required="{Boolean}true"/>

                                                        <background-color
                                                                granite:id="pwa-cloudconfiguration-bg-color"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                                                                fieldDescription="The default color that would display when the application launches from the splash screen"
                                                                fieldLabel="Background Color"
                                                                name="./backgroundColor"
                                                                required="{Boolean}true"/>
                                                        <display-type
                                                                granite:id="pwa-cloudconfiguration-display-type"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                fieldDescription="The application display behavior when launched."
                                                                fieldLabel="Display Type"
                                                                name="./display"
                                                                required="{Boolean}true">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <standalone
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Standalone"
                                                                        value="standalone"/>
                                                                <fullscreen
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Fullscreen"
                                                                        value="fullscreen"/>
                                                                <browser
                                                                        jcr:primaryType="nt:unstructured"
                                                                        text="Browser"
                                                                        value="browser"/>
                                                            </items>
                                                        </display-type>
                                                        <icons
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                fieldLabel="Icons"
                                                                fieldDescription="Click on Add Field to add more Icons. "
                                                                composite="{Boolean}true"
                                                                required="{Boolean}false">
                                                            <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                    name="./icons">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <src
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
                                                                            fieldLabel="Icon Path"
                                                                            name="./fileReference"
                                                                            required="{Boolean}false"/>
                                                                    <size
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                                            fieldLabel="Size"
                                                                            name="./size">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <small
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="144x144"
                                                                                    value="144x144"/>
                                                                            <medium
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="192x192"
                                                                                    value="192x192"/>
                                                                            <large
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    text="512x512"
                                                                                    value="512x512"/>
                                                                        </items>
                                                                    </size>
                                                                </items>
                                                            </field>
                                                        </icons>
                                                    </items>
                                                </container>
                                            </items>
                                        </fixedcolumns>
                                    </items>
                                    <parentConfig jcr:primaryType="nt:unstructured">
                                        <prev
                                                granite:class="foundation-wizard-control"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/anchorbutton"
                                                text="Back">
                                            <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    foundation-wizard-control-action="prev"/>
                                        </prev>
                                        <next
                                                granite:class="foundation-wizard-control"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/button"
                                                disabled="{Boolean}false"
                                                text="Create"
                                                type="submit"
                                                variant="primary">
                                            <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    foundation-wizard-control-action="next"/>
                                        </next>
                                    </parentConfig>
                                </manifest-wizard-container>
                            </items>
                        </wizard>
                    </items>
                </form>
            </items>
        </body>
    </jcr:content>
    <properties
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/siteadmin/admin/createpagewizard/properties"
            template="${requestPathInfo.suffix}"/>
</jcr:root>
