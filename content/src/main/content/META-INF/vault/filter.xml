        <include pattern="/etc/notification/email/rep:policy"/>
        <include pattern="/etc/notification/email/acs-commons"/>
        <include pattern="/etc/notification/email/acs-commons/(.*)?"/>
    </filter>
    <filter root="/content/dam/rep:policy"/>
    <filter root="/content/rep:policy"/>
    <filter root="/etc/workflow/instances/rep:policy"/>
    <filter root="/home/users/rep:policy"/>
