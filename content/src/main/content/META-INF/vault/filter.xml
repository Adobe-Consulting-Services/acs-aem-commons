<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/apps/acs-commons">
        <exclude pattern="/apps/acs-commons/components/utilities/designer/clientlibsmanager/clientlibs/less/generated"/>
