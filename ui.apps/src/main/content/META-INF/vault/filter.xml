        <include pattern="/apps/settings"/>
        <include pattern="/apps/settings/dam"/>
        <include pattern="/apps/settings/dam/indesign"/>
        <include pattern="/apps/settings/dam/indesign/scripts(/.*)?"/>
    </filter>
</workspaceFilter>
