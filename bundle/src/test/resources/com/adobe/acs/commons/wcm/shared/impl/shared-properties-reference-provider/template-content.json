          },
          "footer": {
            "jcr:primaryType": "nt:unstructured",
            "sling:resourceType": "sample-site/components/structure/footer",
            "navigation": {
              "jcr:primaryType": "nt:unstructured",
              "sling:resourceType": "sample-site/components/structure/navigation",
              "collectAllPages": "false",
              "structureDepth": "1"
            }
          }
        },
        "cq:responsive": {
          "jcr:primaryType": "nt:unstructured",
          "breakpoints": {
            "jcr:primaryType": "nt:unstructured",
